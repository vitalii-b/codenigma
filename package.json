{
	"name": "codenigma",
	"displayName": "Codenigma",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.100.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codenigma.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test",
		"package": "node ./node_modules/@vscode/vsce/vsce package"
	},
	"devDependencies": {
		"@types/vscode": "1.100.0",
		"@types/mocha": "10.0.10",
		"@types/node": "20.x",
		"@types/ws": "8.18.1",
		"@typescript-eslint/eslint-plugin": "8.31.1",
		"@typescript-eslint/parser": "8.31.1",
		"eslint": "9.25.1",
		"typescript": "5.8.3",
		"@vscode/test-cli": "0.0.10",
		"@vscode/test-electron": "2.5.2",
		"@vscode/vsce": "3.4.0"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.11.1",
		"zod": "3.24.4",
		"ws": "8.18.2"
	}
}